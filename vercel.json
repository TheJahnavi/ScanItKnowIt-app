{
  "outputDirectory": "dist",
  "buildCommand": "npm run build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/client/dist/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/client/dist/index.html"
    }
  ],
  "functions": {
    "dist/api/index.js": {
      "runtime": "vercel-node@3.2.25"
    }
  }
}